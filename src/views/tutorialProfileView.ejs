<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Profiles - CS+SG Edu Team</title>
        <link rel="stylesheet" href="/static/tutorialProfileView.css">
    </head>
    <body>
        <div class="nav">
            <a href="/tutorial/profiles/pretty"><b>[This is] Prettified Demo</b></a>
            <a href="/tutorial/profiles">Unformatted Demo</a>
        </div>
        <div class="content">
            <h1>CS+SG Education Team Profiles</h1>
            <h2>Meet the Team!</h2>
            <table>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Permalink</th>
                </tr>
                <% for (const profile of profiles) { %>
                    <tr>
                        <td><%= profile.id %></td>
                        <td><%= profile.firstName %></td>
                        <td><%= profile.lastName %></td>
                        <td><a href=<%= "/tutorial/profiles/id/" + profile.id + "/pretty" %>>Link</a></td>
                    </tr>
                <% } %>
            </table>
        </div>
    </body>
</html>